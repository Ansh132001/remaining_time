{
  "name": "addon_template",
  "version": "20.5.4i8",
  "description": "template for trgkanki's addon",
  "scripts": {
    "build": "",
    "update:template": "npx trgkanki-template-cli update",
    "dist": "npm run build && npx trgkanki-template-cli package",
    "test": "npm-run-all test:py",
    "test:py": "nosetests src/tests"
  },
  "keywords": [
    "anki",
    "anki-addon"
  ],
  "author": "trgk",
  "license": "AGPL3",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is --not master && lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ],
    "*.py": [
      "black"
    ]
  },
  "dependencies": {}
}
